#. Plugin Name of the plugin
#: passwp-posts.php
#: includes/class-admin-settings.php:52
msgid "PassWP Posts"
msgstr "PassWP Posts"

#. Plugin URI of the plugin
#: passwp-posts.php
msgid "https://developer.suspended.no/passwp-posts"
msgstr "https://developer.suspended.no/passwp-posts"

#. Description of the plugin
#: passwp-posts.php
msgid "Password protects all pages and posts except the front page. Logged-in users bypass the password."
msgstr "Passordbeskytter alle sider og innlegg unntatt forsiden. Innloggede brukere omgår passordet."

#. Author of the plugin
#: passwp-posts.php
msgid "Per Soderlind"
msgstr "Per Soderlind"

#. Author URI of the plugin
#: passwp-posts.php
msgid "https://soderlind.no"
msgstr "https://soderlind.no"

#: includes/class-admin-settings.php:51
msgid "PassWP Posts Settings"
msgstr "PassWP Posts-innstillinger"

#: includes/class-admin-settings.php:83
msgid "Password Protection Settings"
msgstr "Innstillinger for passordbeskyttelse"

#: includes/class-admin-settings.php:91
msgid "Enable Protection"
msgstr "Aktiver beskyttelse"

#: includes/class-admin-settings.php:100
#: templates/password-form.php:88
msgid "Password"
msgstr "Passord"

#: includes/class-admin-settings.php:109
msgid "Remember Me Duration"
msgstr "Varighet for «Husk meg»"

#: includes/class-admin-settings.php:118
msgid "Protection Mode"
msgstr "Beskyttelsesmodus"

#: includes/class-admin-settings.php:127
msgid "Excluded Pages/Posts"
msgstr "Ekskluderte sider/innlegg"

#: includes/class-admin-settings.php:136
msgid "Protected Pages/Posts"
msgstr "Beskyttede sider/innlegg"

#: includes/class-admin-settings.php:202
msgid "Search for pages or posts..."
msgstr "Søk etter sider eller innlegg..."

#: includes/class-admin-settings.php:203
msgid "Show password"
msgstr "Vis passord"

#: includes/class-admin-settings.php:204
msgid "Hide password"
msgstr "Skjul passord"

#: includes/class-admin-settings.php:228
msgid "Save Settings"
msgstr "Lagre innstillinger"

#: includes/class-admin-settings.php:239
msgid "Configure password protection for your site. The front page is always public. Logged-in users bypass the password."
msgstr "Konfigurer passordbeskyttelse for nettstedet ditt. Forsiden er alltid offentlig. Innloggede brukere omgår passordet."

#: includes/class-admin-settings.php:251
msgid "Enable password protection"
msgstr "Aktiver passordbeskyttelse"

#: includes/class-admin-settings.php:254
msgid "When enabled, visitors must enter a password to view protected content."
msgstr "Når aktivert, må besøkende taste inn et passord for å se beskyttet innhold."

#: includes/class-admin-settings.php:269
msgid "Leave blank to keep current password"
msgstr "La stå tomt for å beholde nåværende passord"

#: includes/class-admin-settings.php:269
#: templates/password-form.php:91
msgid "Enter password"
msgstr "Skriv inn passord"

#: includes/class-admin-settings.php:272
msgid "Toggle password visibility"
msgstr "Veksle passordsynlighet"

#: includes/class-admin-settings.php:278
msgid "A password is currently set. Enter a new password to change it, or leave blank to keep the current password."
msgstr "Et passord er allerede satt. Skriv inn et nytt passord for å endre det, eller la stå tomt for å beholde nåværende passord."

#: includes/class-admin-settings.php:282
msgid "Enter the password visitors will use to access protected content."
msgstr "Skriv inn passordet besøkende skal bruke for å få tilgang til beskyttet innhold."

#: includes/class-admin-settings.php:298
msgid "days"
msgstr "dager"

#: includes/class-admin-settings.php:300
msgid "How long visitors stay authenticated after entering the password."
msgstr "Hvor lenge besøkende forblir autentisert etter å ha tastet inn passordet."

#: includes/class-admin-settings.php:316
msgid "Protect all pages and posts (except front page)"
msgstr "Beskytt alle sider og innlegg (unntatt forsiden)"

#: includes/class-admin-settings.php:322
msgid "Protect only selected pages and posts"
msgstr "Beskytt kun valgte sider og innlegg"

#: includes/class-admin-settings.php:357
msgid "These pages and posts will not require a password."
msgstr "Disse sidene og innleggene vil ikke kreve passord."

#: includes/class-admin-settings.php:392
msgid "Only these pages and posts will require a password."
msgstr "Kun disse sidene og innleggene vil kreve passord."

#: includes/class-admin-settings.php:464
msgid "Permission denied."
msgstr "Tilgang nektet."

#: includes/class-protection.php:158
msgid "Security check failed."
msgstr "Sikkerhetssjekk mislyktes."

#: templates/password-form.php:22
msgid "Incorrect password. Please try again."
msgstr "Feil passord. Vennligst prøv igjen."

#: templates/password-form.php:23
msgid "No password has been configured. Please contact the site administrator."
msgstr "Ingen passord er konfigurert. Vennligst kontakt nettstedets administrator."

#. translators: %s is the site name.
#: templates/password-form.php:35
#, php-format
msgid "Password Required - %s"
msgstr "Passord påkrevd - %s"

#: templates/password-form.php:63
msgid "Password Required"
msgstr "Passord påkrevd"

#: templates/password-form.php:65
msgid "This content is protected. Please enter the password to continue."
msgstr "Dette innholdet er beskyttet. Vennligst skriv inn passordet for å fortsette."

#: templates/password-form.php:98
msgid "Remember me"
msgstr "Husk meg"

#: templates/password-form.php:103
msgid "Submit"
msgstr "Send inn"

#: templates/password-form.php:110
msgid "Back to home"
msgstr "Tilbake til forsiden"
